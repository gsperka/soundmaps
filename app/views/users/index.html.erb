<%= javascript_tag do %>
  window.tracks = <%=raw Track.all.to_json %>
<% end %>

<div id="map-canvas"></div>

<div id='footer'>
  <form id='geocoder'>
    <input id='geocode_bar' id='address' type='textbox' name='location' value='351 W. Hubbard Chicago, IL'>
    <input type='submit' value='Find Location'>
  </form>

  <span id='song'>
    <audio id='player' controls></audio>
  </span>
</div>


<div id="overlay"></div>


<div id="login_modal">
  <%= render '/sessions/login' %>
</div>

<div id="signup_modal">
  <%= render '/users/new' %>
</div>

<div id="audio_modal">
  <%= render '/tracks/new' %>
</div>




























